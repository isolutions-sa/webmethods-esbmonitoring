</select>
</TD>
<TD align = "right">Service Name:</TD>
<TD align = "left">
<input name="SERVICENAME" size="50" type="text">
</TD>
</TR>
<TR>
<TD align = "right">Server Name:</TD>
<TD align = "left">
<select name="SERVERNAME">
	<option value="" selected></option>
	%invoke AcCommon.logging.userInterface:getRemoteServersList%
	%loop Servers%
	<option value="%value Host%">%value Host%</option>
	%end%
</select>
</TD>
<TD align = "right">Context ID:</TD>
<TD align = "left">
<input name="CONTEXTID" size="50" type="text">
</TD>
</TR>
<TR>
<TD align = "right">Root Context ID:</TD>
<TD align = "left">
<input name="ROOTCONTEXTID" size="50" type="text">
</TD>
<TD align = "right">Parent Context ID:</TD>
<TD align = "left">
<input name="PARENTCONTEXTID" size="50" type="text">
</TD>
</TR>
<TR>
<TD align = "right">Current Status:</TD>
<TD align = "left">
<select size="3" name="CURRENTSTATUS">
<option value="" selected></option> 
 <option value="COMPLETED">Completed</option>
  <option value="FAILED">Failed</option>
  </select>
</TD>
<TD align = "right">Activity Status:</TD>
